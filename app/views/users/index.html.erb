<h1>All Users</h1>
<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td>
          <%= link_to 'Show', user, class: 'btn' %> |
          <%= link_to 'Edit', edit_user_path(user), class: 'btn' %> |
          <%= link_to 'Delete', user, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'Cities', cities_path, class: 'btn' %>
<%= link_to 'New User', new_user_path, class: 'btn' %>
<%= link_to 'Login', new_user_session_path, class: 'btn' %>
<%= link_to 'Log off', destroy_user_session_path, method: :delete, class: 'btn' %>



